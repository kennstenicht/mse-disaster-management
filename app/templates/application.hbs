{{#each map in maps}}
  {{#if map.baseMap}}
    {{mapbox-gl-map settings=map tasks=tasks}}
  {{else}}
    {{#if map.active}}
      {{#lence-overlay settings=map isVisible=map.active}}
        {{mapbox-gl-map settings=map tasks=tasks}}
      {{/lence-overlay}}
    {{/if}}
  {{/if}}
{{/each}}


{{outlet}}
